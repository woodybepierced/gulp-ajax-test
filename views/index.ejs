<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
    
    
  </head>
  <body>
    <h1><%= title %></h1>
    <p>Welcome to <%= title %></p>
    <button class="button js-ajax-button">
      <span class="button-text">Click Me</span>
      <span class="loading-state"></span>
    </button>

    <div class="usersWrapper__row">

      <div class="usersWrapper">
        <h3>Users AJAX'd w/ createElement & appendChild</h3>
        <div class="js-users"></div>
      </div>

      <div class="usersWrapper">
        <h3>Users AJAX'd w/ Template</h3>
        <div class="js-templating-users"></div>
      </div>
    </div>
    
    <script type="text/template" id="user-template">
      <div class="user" id="{{id}}">
        <h3>{{name}}</h3>
        <p>{{email}}</p>
      </div>
    </script>
    <script type='text/javascript' src='/js/ajax.js'></script>
    <!-- Compile/Minify the JS files into one distribution file -->
  </body>
</html>

